# Server Configuration
PORT=8080
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/kakao_relay

# Relay Configuration
RELAY_BASE_URL=https://relay.example.com

# Kakao Configuration (optional)
KAKAO_SIGNATURE_SECRET=

# Queue/Polling Settings
QUEUE_TTL_SECONDS=900
MAX_POLL_WAIT_SECONDS=30
CALLBACK_TTL_SECONDS=55

# Rate Limiting
DEFAULT_RATE_LIMIT_PER_MINUTE=60

# Logging
LOG_LEVEL=info
